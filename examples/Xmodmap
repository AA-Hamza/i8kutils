! /etc/X11/Xmodmap

! Multimedia buttons on Inspiron 8000, used by sawfish to control xmms
keycode 129 = F31
keycode 130 = F32
keycode 131 = F33
keycode 132 = F34

! Fn key and Volume buttons, available only with i8k handle_buttons=1
keycode 160 = F27
keycode 174 = F28
keycode 176 = F29

! end of file
